{
    "name": "imjonos/laravel-json-api-client",
    "description": "version 0.1a",
    "license": "MIT",
    "authors": [
        {
            "name": "Eugeny Nosenko",
            "email": "info@toprogram.ru",
            "homepage": "https://toprogram.ru"
        }
    ],
    "homepage": "https://github.com/laravel-json-api-client",
    "keywords": ["Laravel", "JsonApiClient"],
    "require": {
        "illuminate/support": "~5|~6|~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Nos\\JsonApiClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nos\\JsonApiClient\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nos\\JsonApiClient\\JsonApiClientServiceProvider"
            ],
            "aliases": {
                "JsonApiClient": "Nos\\JsonApiClient\\Facades\\JsonApiClient"
            }
        }
    }
}
