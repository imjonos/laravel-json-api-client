{
    "name": "nos/jsonapiclient",
    "description": ":package_description",
    "license": "license",
    "authors": [
        {
            "name": "author name",
            "email": "author email",
            "homepage": "author homepage"
        }
    ],
    "homepage": "https://github.com/imjonos/jsonapiclient",
    "keywords": ["Laravel", "JsonApiClient"],
    "require": {
        "illuminate/support": "~5|~6|~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Nos\\JsonApiClient\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nos\\JsonApiClient\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nos\\JsonApiClient\\JsonApiClientServiceProvider"
            ],
            "aliases": {
                "JsonApiClient": "Nos\\JsonApiClient\\Facades\\JsonApiClient"
            }
        }
    }
}
